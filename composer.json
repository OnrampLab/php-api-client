{
    "name": "onramplab/php-api-client",
    "description": "An basic api client to be extended",
    "type": "library",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Kos Huang",
            "email": "kos.huang@onramplab.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable":true,
    "require": {
        "php": "^7.1.3|^8.0",
        "guzzlehttp/guzzle": "^6.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9.5",
        "sempro/phpunit-pretty-print": "^1.4",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "OnrampLab\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onramplab\\ApiClient\\Tests\\":"tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
